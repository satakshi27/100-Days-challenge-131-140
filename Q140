Q140:Define a struct with enum Gender and print person's gender.

#include <stdio.h>

typedef enum {
    MALE,
    FEMALE,
    OTHER
} 
Gender;

typedef struct
{
    char name[50];
    Gender gender;
} 
Person;

const char* getGenderName(Gender g)
{
    switch (g) {
        case MALE:   return "Male";
        case FEMALE: return "Female";
        case OTHER:  return "Other";
        default:     return "Unknown";
    }
}

int main()
{
    Person p1 = {"Alice", FEMALE};
    Person p2 = {"Bob", MALE};
    Person p3 = {"Sam", OTHER};
    printf("%s is %s.\n", p1.name, getGenderName(p1.gender));
    printf("%s is %s.\n", p2.name, getGenderName(p2.gender));
    printf("%s is %s.\n", p3.name, getGenderName(p3.gender));

    return 0;
}
