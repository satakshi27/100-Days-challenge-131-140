Q138:Print all enum names and integer values using a loop.

#include <stdio.h>

typedef enum 
{
    ADMIN,
    USER,
    GUEST,
    ROLE_COUNT  
} UserRole;

int main()
{
    const char *roleNames[] = {"ADMIN", "USER", "GUEST"};

    for (int i = 0; i < ROLE_COUNT; i++) {
        printf("Role: %s, Value: %d\n", roleNames[i], i);
    }

    return 0;
}
